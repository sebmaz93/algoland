{
    "clearMocks": true,
    "reporters": [ "default", "jest-junit" ],
    "coverageThreshold": {
        "global": {
            "branches": 1,
            "functions": 1,
            "lines": 1,
            "statements": -1
        }
    },
    "moduleNameMapper": {
        "@code/(.*)": [
            "<rootDir>/src/ts/$1"
        ]
    },
    "preset": "ts-jest"
}